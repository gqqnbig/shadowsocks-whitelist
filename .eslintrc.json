{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
		"no-tabs": 0,
		"indent": ["error", "tab"],
		"linebreak-style": 0,
		"no-unused-vars": ["error", { "varsIgnorePattern": "FindProxyForURL" }],
		"pac": ["error", require("./custom-rules/enforce-userrules-sorting")],
    }
}
